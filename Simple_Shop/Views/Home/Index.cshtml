@model IEnumerable<Product>
@{
    ViewData["Title"] = "Shop";
}

<div class="text-center">
    <h1 class="display-4">Welcome To Simple Eshop</h1>
    <hr />
    <div class="row">
        @foreach (var item in Model)
        {
            <partial name="_BoxProduct" model="@item" />

        }
    </div>
</div>
